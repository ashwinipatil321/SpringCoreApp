package com.autowire.model;

public class ClassA {
	
	ClassB bobject;
	public ClassA() {
		System.out.println("a is created");
	}
	public ClassB getBobject() {
		return bobject;
	}
	public void setBobject(ClassB bobject) {
		this.bobject = bobject;
	}
	void print(){
		System.out.println("hello a");
	}  
	public void display(){  
		print();  
		bobject.print();  
	}  
}
